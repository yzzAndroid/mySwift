<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qianfeng.yyz.myswift">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SecondActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".mvp.view.LogInActivity" />
        <activity android:name=".mvp.view.SignInActivity" />
        <activity android:name=".activity.MyGiftActivity" />
        <activity android:name=".activity.SendEmailActivity" />
        <activity android:name=".activity.AboutUsActivity" />
        <activity android:name=".activity.SpecialLeftDetalActivity" />
        <activity android:name=".activity.SpecialRightDetalActivity"></activity>
    </application>

</manifest>